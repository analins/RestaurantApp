<%= button_to "Log Out", sessions_path, method: :delete %>
<h1>Staff</h1>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.username %> <%= button_to "x", user_path(user), method: :delete%>
    </li>
  <% end %>
</ul>

<%= form_for @user do |f| %>
  <%= f.label 'Name' %>
  <%= f.text_field :username %>

  <%= f.submit %>
<% end %>
