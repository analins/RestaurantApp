<%= stylesheet_link_tag    'login', media: 'all', 'data-turbolinks-track' => true %>
<h2>Staff Login</h2>

  <%= form_tag users_path do %>
    <%= label_tag(:user) %>
    <%= collection_select(:user, :user_id, User.all, :id, :name)%> <br>
    <%=link_to 'Login', users_path, class: 'waves-effect waves-light deep-purple btn' %>
    <% end %>
